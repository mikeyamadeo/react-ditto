{
  "name": "mikeyamadeo",
  "version": "0.0.1",
  "description": "Component based layout and spacing utilities for rendering UI with react",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel --copy-files --out-dir dist src",
    "prebuild": "rimraf dist",
    "test": "ava -w",
    "examples": "webpack-dev-server --inline --content-base examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikeyamadeo/react-ditto.git"
  },
  "keywords": [
    "react",
    "trumps",
    "utility",
    "layout"
  ],
  "author": "Mikey Murphy <mikeyamadeo@gmail.com> (https://twitter.com/TranscendMikey)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikeyamadeo/react-ditto/issues"
  },
  "homepage": "https://github.com/mikeyamadeo/react-ditto#readme",
  "dependencies": {
    "aphrodite": "0.5.0",
    "react": "15.1.0"
  },
  "devDependencies": {
    "asap": "2.0.4",
    "ava": "0.16.0",
    "babel-cli": "6.10.1",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "enzyme": "2.4.1",
    "file-loader": "0.9.0",
    "jsdom": "9.4.2",
    "react-addons-test-utils": "15.3.1",
    "react-dom": "15.1.0",
    "rimraf": "2.5.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ]
  },
  "ava": {
    "files": [
      "**/spec.js",
      "**/*.spec.js"
    ],
    "require": [
      "babel-register",
      "./spec/setup.js"
    ],
    "babel": "inherit"
  },
  "standard": {
    "showEslintRules": true,
    "parser": "babel-eslint",
    "rules": {
      "strict": 0
    }
  }
}
